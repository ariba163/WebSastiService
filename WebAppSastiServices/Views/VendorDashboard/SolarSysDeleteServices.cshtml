
@{
    ViewBag.Title = "DeleteServices";
    Layout = "~/Views/Shared/_LayoutDashboardSolarSysVendor.cshtml";
}

<!-- start page inner -->
    <div class="page-inner">
        <div class="page-title">
            <h3 class="breadcrumb-header">Delete Service</h3>
        </div>
        <!-- start page main wrapper -->
        <div id="main-wrapper">
            <div class="row">
                <div class="col-md-12">
                    <div class="card card-white">
                        <div class="card-heading clearfix">
                            <h4 class="card-title">Are u sure u want to delete?</h4>
                        </div>
                        <div class="card-body">

                            <div id="rootwizard">
                                <div class="progress progress-sm m-t-sm">
                                    <div class="progress-bar progress-bar-info progress-bar-striped active" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                                    </div>
                                </div>
                                <form>
                                    <div>
                                        <div class=" fade show active" id="home3">
                                            <div class="row m-b-lg">
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="form-group col-md-6">
                                                            <label for="ServiceName">Service Name</label>
                                                            <input type="text" class="form-control" name="ServiceName" id="ServiceName" placeholder="Service Name" disabled>
                                                        </div>
                                                        <div class="form-group col-md-6 form-row">
                                                            <label for="ItemsName" class="col-12">Items Name</label>
                                                            <input type="text" class="form-control" name="ItemName" id="ItemName" placeholder="oil, filters, engine" disabled>
                                                        </div>
                                                        <div class="form-group col-md-6">
                                                            <label for="FuelType">Fuel type</label>
                                                            <select class="form-control" id="FuelType" disabled>
                                                                <option>Petrol</option>
                                                                <option>Gas</option>
                                                                <option>Diesal</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-md-6">
                                                            <label for="Power">Power</label>
                                                            <select class="form-control" id="Power" disabled>
                                                                <option>Range   - unit</option>
                                                                <option>Range   - unit</option>
                                                                <option>Range   - unit</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label for="Rate">Service Rate</label>
                                                            <input type="number" class="form-control" name="Rate" id="Rate" placeholder="Service Rate" disabled>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <ul class="pagination wizard">
                                            <li class="previous"><a href="#" class="btn btn-outline-primary">Previous</a></li>
                                            <li class="next ml-auto"><a href="#" class="btn btn-outline-danger">Delete</a></li>
                                        </ul>
                                    </div>
                                </form>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <!-- Row -->
        </div>
        <!-- end page main wrapper -->
        <div class="page-footer">
            <p>
                Copyright &copy;
                <script>document.write(new Date().getFullYear())</script> Crizal All rights reserved.
            </p>
        </div>
    </div>
<!-- end page inner -->
