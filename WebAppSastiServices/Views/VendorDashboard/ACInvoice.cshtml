                                                </tr>
                                                <tr>
                                                    <th scope="row">6</th>
                                                    <td>Table cell</td>
                                                    <td>Table cell</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Row -->

            </div>
            <!-- Row -->
            <div class="row">
                <div class="col-md-12" id="Bill">
                    <div class="row">
                        <div class="col-md-9">
                            <div class="card card-white">
                                <div class="card-heading clearfix">
                                    <h4 class="card-title">Total Charges</h4>
                                </div>
                                <div class="card-body">

                                    <div class="table-responsive">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Description</th>
                                                    <th>Charges</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <th scope="row">1</th>
                                                    <td>Service Charges</td>
                                                    <td>20000</td>
                                                </tr>
                                                <tr>
                                                    <th scope="row">2</th>
                                                    <td>Item Charges</td>
                                                    <td>90000</td>
                                                </tr>
                                                <tr>
                                                    <th scope="row">3</th>
                                                    <td>Asaan Service Charges</td>
                                                    <td>2000</td>
                                                </tr>
                                                <tr class="border-bottom border-double border-color-dark-gray">
