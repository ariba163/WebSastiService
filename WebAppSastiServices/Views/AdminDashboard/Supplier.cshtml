
@{
    ViewBag.Title = "Supplier";
    Layout = "~/Views/Shared/_LayoutDashboardAdmin.cshtml";
}


<!-- start page inner -->
<div class="page-inner">
    <div class="page-title">
        <h3 class="breadcrumb-header">Supplier</h3>
    </div>
    <!-- start page main wrapper -->
    <div id="main-wrapper">
        <div class="row">
            <a href="@Url.Action("GeneratorsIndex","SupplierDashboard")" class="col-xl-4 col-lg-6 col-md-6">
                <div class="card card-white stats-widget">
                    <div class="card-body">
                        <div class="float-left">
                            <span class="stats-number">Generators</span>
                            <p class="stats-info">813 Orders</p>
                        </div>
                        <div class="clearfix"></div>
                        <div class="progress mb-0 mt-3">
                            <div class="progress-bar bg-primary" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
                                <span class="sr-only">40% Complete</span>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="@Url.Action("ACIndex","SupplierDashboard")" class="col-xl-4 col-lg-6 col-md-6">
                <div class="card card-white stats-widget">
                    <div class="card-body">
                        <div class="float-left">
                            <span class="stats-number">Air Conditioners</span>
                            <p class="stats-info">178 Orders</p>
                        </div>
                        <div class="clearfix"></div>
                        <div class="progress mb-0 mt-3">
                            <div class="progress-bar bg-danger" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 20%;">
                                <span class="sr-only">20% Complete</span>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="@Url.Action("UPSIndex","SupplierDashboard")" class="col-xl-4 col-lg-6 col-md-6">
                <div class="card card-white stats-widget">
                    <div class="card-body">
                        <div class="float-left">
                            <span class="stats-number">UPS</span>
                            <p class="stats-info">888 Orders</p>
                        </div>
                        <div class="clearfix"></div>
                        <div class="progress mb-0 mt-3">
                            <div class="progress-bar bg-warning" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%;">
                                <span class="sr-only">70% Complete</span>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="@Url.Action("SolarSysIndex","SupplierDashboard")" class="col-xl-4 col-lg-6 col-md-6">
                <div class="card card-white stats-widget">
                    <div class="card-body">
                        <div class="float-left">
                            <span class="stats-number">Solar System</span>
                            <p class="stats-info">58 Orders</p>
                        </div>
                        <div class="clearfix"></div>
                        <div class="progress mb-0 mt-3">
                            <div class="progress-bar bg-success" role="progressbar" aria-valuenow="58" aria-valuemin="0" aria-valuemax="100" style="width: 58%;">
                                <span class="sr-only">58% Complete</span>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="@Url.Action("CCTVIndex","SupplierDashboard")" class="col-xl-4 col-lg-6 col-md-6">
